    "formidable": "^1.2.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "sqlite3": "^4.0.0"
  },
  "devDependencies": {
    "electron-rebuild": "^1.7.3"
  },
  "engines": {
    "node": ">= 8"
