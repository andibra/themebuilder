{
  "name": "themebuilder",
  "version": "1.0.4",
  "description": "A Bootstrap theme builder.",
  "main": "index.js",
  "scripts": {
    "start": "electron index.js",
    "setup": "npm install | npm run clone-bootstrap && npm run init-custom",
    "clone-bootstrap": "node ./lib/tasks/git_clone_bootstrap.js",
    "init-custom": "node ./lib/tasks/init_custom_scss.js",
    "compile-scss": "sass --style=compressed ./tmp/custom.scss ./app/assets/css/bootstrap_theme.min.css"
  },
  "keywords": [
    "css",
    "sass",
    "bootstrap-styles",
    "bootstrap",
    "theme-builders",
    "electron"
  ],
  "dependencies": {
    "debug": "^3.1.0",
    "ejs": "^2.5.9",
    "electron": "^2.0.0",
    "express": "^4.16.2",
    "font-awesome": "^4.7.0",
    "formidable": "^1.2.1",
    "hoek": "^4.2.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "root-model": "^1.0.0"
  },
  "engines": {
    "node": ">= 8"
  },
  "author": "Andy Bettisworth <andy@accreu.com> https://andybettisworth.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wurde/themebuilder.git"
  },
  "bugs": {
    "url": "https://github.com/wurde/themebuilder/issues"
  }
}
